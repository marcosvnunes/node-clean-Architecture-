{
  "name": "clean-node-api",
  "version": "1.3.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.2.3",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.16",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.10",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.1",
    "@typescript-eslint/eslint-plugin": "3",
    "coveralls": "^3.1.0",
    "eslint": "7",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-standard": "4",
    "git-commit-msg-linter": "^3.0.0",
    "globby": "^11.0.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.2",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "node dist/main/server.js",
    "build": "rimraf dist && tsc",
    "dev": "tsc -w",
    "debug": "nodemon -L --watch ./dist --inspect=0.0.0.0:9222 --nolazy ./dist/main/server.js",
    "up": "yarn build && docker-compose up -d",
    "down": "docker-compose down",
    "test": "jest --passWithNoTests --silent --noStackTrace ",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:integration": "yarn test -- --watch --runInBand -c jest.integration.config.js",
    "test:unit": "yarn test -- --watch -c jest.unit.config.js",
    "test:staged": "yarn test -- --findRelatedTests",
    "test:ci": "yarn test --coverage --runInBand",
    "test:coveralls": "yarn test:ci && coveralls < coverage/lcov.info"
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "bcrypt": "^5.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.3",
    "nodemon": "^2.0.6",
    "validator": "^13.5.1"
  },
  "engines": {
    "node": "12.x || 15.x"
  }
}
